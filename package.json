{
  "name": "auth",
  "version": "0.0.1",
  "description": "Nest.js user auth playground",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "ts-node -r dotenv/config ./src/cli.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js",
    "typeorm:migration:create": "npm run typeorm -- migration:create -n",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "typeorm:migration:run": "npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^7.0.7",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/mongoose": "^6.4.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/schedule": "^0.3.1",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^7.0.0",
    "argon2": "^0.26.1",
    "bson-objectid": "^1.3.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "date-fns": "^2.11.0",
    "fs-extra": "^9.0.0",
    "helmet": "^3.22.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.0",
    "memcached": "^2.2.2",
    "mongoose": "^5.9.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "rate-limiter-flexible": "^2.1.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.0",
    "sharp": "^0.25.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@types/express": "^4.17.2",
    "@types/mongoose": "^5.7.7",
    "@types/node": "^13.1.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sharp": "^0.24.0",
    "@types/uuid": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
